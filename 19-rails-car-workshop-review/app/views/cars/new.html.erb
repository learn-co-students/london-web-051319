<h2>new car</h2>

<%= form_for @car do |f| %>
    <%= f.label :brand_id %>
    <%= f.collection_select :brand_id, Brand.all, :id, :name %>
    <%= f.label :owner_id  %>
    <%= f.collection_select :owner_id, Owner.all, :id, :name %>
    <%= f.label :mechanic_id %>
    <%= f.collection_select :mechanic_id, Mechanic.all, :id, :name %>
    <%= f.label :mileage %>
    <%= f.number_field :mileage %>
    <%= f.submit %>
<% end %>